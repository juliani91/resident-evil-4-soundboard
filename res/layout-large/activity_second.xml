<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.residentevil4ganadossoundboard.SecondActivity"
    android:background="@drawable/cultist" >

    <Button
        android:id="@+id/button14"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="25dp"
        android:text="@string/cabeza"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button15"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_alignLeft="@+id/button14"
        android:layout_below="@+id/button14"
        android:layout_marginTop="16dp"
        android:text="@string/cabron"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button16"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_alignRight="@+id/button15"
        android:layout_below="@+id/button15"
        android:layout_marginTop="16dp"
        android:text="@string/cabron2"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_alignRight="@+id/button16"
        android:layout_below="@+id/button16"
        android:layout_marginTop="20dp"
        android:text="@string/cerebro"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_alignRight="@+id/button17"
        android:layout_below="@+id/button17"
        android:layout_marginTop="22dp"
        android:text="@string/cogedlo"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button19"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_alignLeft="@+id/button18"
        android:layout_below="@+id/button18"
        android:layout_marginTop="22dp"
        android:text="@string/cogedlo2"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button20"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_above="@+id/button15"
        android:layout_alignParentRight="true"
        android:layout_marginRight="49dp"
        android:text="@string/cogedlo3" 
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp"/>

    <Button
        android:id="@+id/button21"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_above="@+id/button16"
        android:layout_alignLeft="@+id/button20"
        android:text="@string/cogedlo4"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp"/>

    <Button
        android:id="@+id/button22"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_above="@+id/button17"
        android:layout_alignLeft="@+id/button21"
        android:text="@string/escape"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button23"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_above="@+id/button18"
        android:layout_alignLeft="@+id/button22"
        android:text="@string/estaherido"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button24"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_above="@+id/button19"
        android:layout_alignLeft="@+id/button23"
        android:text="@string/forastero"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/button25"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_alignBaseline="@+id/button19"
        android:layout_alignBottom="@+id/button19"
        android:layout_alignLeft="@+id/button24"
        android:text="@string/trampa"
        android:background="#cc000000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <Button
        android:id="@+id/next2"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_above="@+id/textView2"
        android:layout_alignLeft="@+id/button25"
        android:text="@string/next"
        android:background="#ccff0000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/next2"
        android:layout_marginBottom="35dp"
        android:layout_marginRight="38dp"
        android:text="@string/two"
        android:textSize="24sp" />

    <Button
        android:id="@+id/back2"
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:layout_above="@+id/textView2"
        android:layout_alignLeft="@+id/button19"
        android:text="@string/back"
        android:background="#ccff0000"
        android:textColor="#FFFFFF"
        android:textSize="24sp" />

</RelativeLayout>
